{"ast":null,"code":"import \"core-js/modules/es.array.for-each.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/web.dom-collections.for-each.js\";\nimport \"core-js/modules/es.function.name.js\";\nexport default {\n  props: {\n    locations: {\n      type: Array,\n      \"default\": function _default() {\n        return [];\n      }\n    }\n  },\n  mounted: function mounted() {\n    var script = document.createElement('script');\n    //script.src = 'http://api.map.baidu.com/api?v=3.0&ak=yqSmodCacSjq9yKQbhZNAGGi2l7nrLtk';\n    script.src = 'http://api.map.baidu.com/api?v=3.0&ak=6p5MmRood0q6FYqDDF9FQhXOM3obsUn7';\n    script.onload = this.initializeMap;\n    document.body.appendChild(script);\n  },\n  methods: {\n    initializeMap: function initializeMap() {\n      var map = new window.BMap.Map(this.$refs.mapContainer);\n\n      // Set default center and zoom level\n      var defaultCenter = new window.BMap.Point(120, 30);\n      map.centerAndZoom(defaultCenter, 15);\n\n      // Add zoom control\n      map.addControl(new window.BMap.NavigationControl());\n\n      // Add markers for each location\n      this.locations.forEach(function (location) {\n        var point = new window.BMap.Point(location.lng, location.lat);\n        var marker = new window.BMap.Marker(point);\n        map.addOverlay(marker);\n        var infoWindow = new window.BMap.InfoWindow(location.name);\n        marker.addEventListener(\"click\", function () {\n          this.openInfoWindow(infoWindow);\n        });\n        marker.openInfoWindow(infoWindow);\n      });\n    }\n  }\n};","map":{"version":3,"names":["props","locations","type","Array","_default","mounted","script","document","createElement","src","onload","initializeMap","body","appendChild","methods","map","window","BMap","Map","$refs","mapContainer","defaultCenter","Point","centerAndZoom","addControl","NavigationControl","forEach","location","point","lng","lat","marker","Marker","addOverlay","infoWindow","InfoWindow","name","addEventListener","openInfoWindow"],"sources":["C:\\Users\\56806\\Desktop\\DBDesign\\src\\components\\BaiduMap.vue"],"sourcesContent":["<!-- \r\n   <template>\r\n    <div class=\"baidu-map\" ref=\"mapContainer\"></div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    mounted() {\r\n      const script = document.createElement('script');\r\n      script.src = 'http://api.map.baidu.com/api?v=3.0&ak=yqSmodCacSjq9yKQbhZNAGGi2l7nrLtk';\r\n      script.onload = this.initializeMap;\r\n      document.body.appendChild(script);\r\n    },\r\n    methods: {\r\n      initializeMap() {\r\n        // 创建地图实例\r\n        const map = new window.BMap.Map(this.$refs.mapContainer); // 使用window.BMap\r\n  \r\n        // 设置中心点坐标和地图级别\r\n        const point = new window.BMap.Point(116.404, 39.915); // 使用window.BMap\r\n        map.centerAndZoom(point, 15);\r\n  \r\n        // 添加标记和信息窗口\r\n        const marker = new window.BMap.Marker(point); // 创建标记\r\n        map.addOverlay(marker); // 添加标记到地图\r\n  \r\n        const location = \"Suzhou\"; // 从滚动框中获取Location元素的内容\r\n        const infoWindow = new window.BMap.InfoWindow(location); // 创建信息窗口\r\n        marker.addEventListener(\"click\", function () {\r\n          this.openInfoWindow(infoWindow); // 点击标记时打开信息窗口\r\n        });\r\n        marker.openInfoWindow(infoWindow); // 默认显示信息窗口\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .baidu-map {\r\n    width: 100%;\r\n    height: 80%;\r\n  }\r\n  </style> -->\r\n \r\n\r\n\r\n\r\n\r\n\r\n<!-- 实现广州啥的显示 -->\r\n   <template>\r\n    <div class=\"baidu-map\" ref=\"mapContainer\"></div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    props: {\r\n      locations: {\r\n        type: Array,\r\n        default: () => []\r\n      }\r\n    },\r\n    mounted() {\r\n      const script = document.createElement('script');\r\n      //script.src = 'http://api.map.baidu.com/api?v=3.0&ak=yqSmodCacSjq9yKQbhZNAGGi2l7nrLtk';\r\n      script.src = 'http://api.map.baidu.com/api?v=3.0&ak=6p5MmRood0q6FYqDDF9FQhXOM3obsUn7';\r\n      script.onload = this.initializeMap;\r\n      document.body.appendChild(script);\r\n    },\r\n    methods: {\r\n      initializeMap() {\r\n        const map = new window.BMap.Map(this.$refs.mapContainer);\r\n  \r\n        // Set default center and zoom level\r\n        const defaultCenter = new window.BMap.Point(120, 30);\r\n        map.centerAndZoom(defaultCenter, 15);\r\n  \r\n        // Add zoom control\r\n        map.addControl(new window.BMap.NavigationControl());\r\n  \r\n        // Add markers for each location\r\n        this.locations.forEach(location => {\r\n          const point = new window.BMap.Point(location.lng, location.lat);\r\n          const marker = new window.BMap.Marker(point);\r\n          map.addOverlay(marker);\r\n  \r\n          const infoWindow = new window.BMap.InfoWindow(location.name);\r\n          marker.addEventListener(\"click\", function () {\r\n            this.openInfoWindow(infoWindow);\r\n          });\r\n          marker.openInfoWindow(infoWindow);\r\n        });\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .baidu-map {\r\n    width: 100%;\r\n    height: 80%;\r\n  }\r\n  </style>\r\n  \r\n\r\n<!-- \r\n\r\n   <template>\r\n    <div class=\"baidu-map\" ref=\"mapContainer\"></div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    props: {\r\n      locations: {\r\n        type: Array,\r\n        default: () => []\r\n      }\r\n    },\r\n    mounted() {\r\n      const script = document.createElement('script');\r\n      script.src = 'http://api.map.baidu.com/api?v=3.0&ak=yqSmodCacSjq9yKQbhZNAGGi2l7nrLtk';\r\n      //script.src = 'http://api.map.baidu.com/api?v=3.0&ak=6p5MmRood0q6FYqDDF9FQhXOM3obsUn7';\r\n      script.onload = this.initializeMap;\r\n      document.body.appendChild(script);\r\n    },\r\n    methods: {\r\n      initializeMap() {\r\n        const map = new window.BMap.Map(this.$refs.mapContainer);\r\n        map.centerAndZoom(\"中国\", 5); // Set initial center and zoom level for China\r\n  \r\n        // Add zoom control\r\n        map.addControl(new window.BMap.NavigationControl());\r\n  \r\n        // Search and mark each location on the map\r\n        this.locations.forEach(location => {\r\n          this.searchLocation(map, location);\r\n        });\r\n      },\r\n      searchLocation(map, location) {\r\n        const localSearch = new window.BMap.LocalSearch(map, {\r\n          onSearchComplete: results => {\r\n            if (localSearch.getStatus() === window.BMAP_STATUS_SUCCESS) {\r\n              const marker = new window.BMap.Marker(results.getPoi(0).point);\r\n              map.addOverlay(marker);\r\n            }\r\n          }\r\n        });\r\n  \r\n        localSearch.search(location);\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .baidu-map {\r\n    width: 100%;\r\n    height: 80%;\r\n  }\r\n  </style>\r\n   -->\r\n\r\n<!-- \r\n   <template>\r\n    <div class=\"baidu-map\" ref=\"mapContainer\"></div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    props: {\r\n      locations: {\r\n        type: Array,\r\n        default: () => []\r\n      }\r\n    },\r\n    mounted() {\r\n      const script = document.createElement('script');\r\n      script.src = 'http://api.map.baidu.com/api?v=3.0&ak=6p5MmRood0q6FYqDDF9FQhXOM3obsUn7';\r\n      script.onload = this.initializeMap;\r\n      document.body.appendChild(script);\r\n    },\r\n    methods: {\r\n      initializeMap() {\r\n        const map = new window.BMap.Map(this.$refs.mapContainer);\r\n        map.centerAndZoom(\"中国\", 5); // Set initial center and zoom level for China\r\n  \r\n        // Add zoom control\r\n        map.addControl(new window.BMap.NavigationControl());\r\n  \r\n        // Search and mark each location on the map\r\n        this.locations.forEach(location => {\r\n          this.searchLocation(map, location);\r\n        });\r\n      },\r\n      searchLocation(map, location) {\r\n        const localSearch = new window.BMap.LocalSearch(map, {\r\n          onSearchComplete: results => {\r\n            if (localSearch.getStatus() === window.BMAP_STATUS_SUCCESS) {\r\n              const marker = new window.BMap.Marker(results.getPoi(0).point);\r\n              map.addOverlay(marker);\r\n            }\r\n          }\r\n        });\r\n  \r\n        localSearch.search(location);\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .baidu-map {\r\n    width: 100%;\r\n    height: 80%;\r\n  }\r\n  </style>\r\n\r\n -->\r\n"],"mappings":";;;;AAuDE,eAAe;EACbA,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,KAAK;MACX,WAAS,SAAAC,SAAA;QAAA,OAAM,EAAC;MAAA;IAClB;EACF,CAAC;EACDC,OAAO,WAAAA,QAAA,EAAG;IACR,IAAMC,MAAK,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ,CAAC;IAC/C;IACAF,MAAM,CAACG,GAAE,GAAI,wEAAwE;IACrFH,MAAM,CAACI,MAAK,GAAI,IAAI,CAACC,aAAa;IAClCJ,QAAQ,CAACK,IAAI,CAACC,WAAW,CAACP,MAAM,CAAC;EACnC,CAAC;EACDQ,OAAO,EAAE;IACPH,aAAa,WAAAA,cAAA,EAAG;MACd,IAAMI,GAAE,GAAI,IAAIC,MAAM,CAACC,IAAI,CAACC,GAAG,CAAC,IAAI,CAACC,KAAK,CAACC,YAAY,CAAC;;MAExD;MACA,IAAMC,aAAY,GAAI,IAAIL,MAAM,CAACC,IAAI,CAACK,KAAK,CAAC,GAAG,EAAE,EAAE,CAAC;MACpDP,GAAG,CAACQ,aAAa,CAACF,aAAa,EAAE,EAAE,CAAC;;MAEpC;MACAN,GAAG,CAACS,UAAU,CAAC,IAAIR,MAAM,CAACC,IAAI,CAACQ,iBAAiB,CAAC,CAAC,CAAC;;MAEnD;MACA,IAAI,CAACxB,SAAS,CAACyB,OAAO,CAAC,UAAAC,QAAO,EAAK;QACjC,IAAMC,KAAI,GAAI,IAAIZ,MAAM,CAACC,IAAI,CAACK,KAAK,CAACK,QAAQ,CAACE,GAAG,EAAEF,QAAQ,CAACG,GAAG,CAAC;QAC/D,IAAMC,MAAK,GAAI,IAAIf,MAAM,CAACC,IAAI,CAACe,MAAM,CAACJ,KAAK,CAAC;QAC5Cb,GAAG,CAACkB,UAAU,CAACF,MAAM,CAAC;QAEtB,IAAMG,UAAS,GAAI,IAAIlB,MAAM,CAACC,IAAI,CAACkB,UAAU,CAACR,QAAQ,CAACS,IAAI,CAAC;QAC5DL,MAAM,CAACM,gBAAgB,CAAC,OAAO,EAAE,YAAY;UAC3C,IAAI,CAACC,cAAc,CAACJ,UAAU,CAAC;QACjC,CAAC,CAAC;QACFH,MAAM,CAACO,cAAc,CAACJ,UAAU,CAAC;MACnC,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}
.$emit('option-selected', option);\r\n      }\r\n    },\r\n    handleOptionSelected(option) {\r\n        console.log('选中的存储容量:', option);\r\n        // 在这里处理选中的存储容量\r\n      },\r\n    components: {\r\n    \"seach\": header,\r\n    StorageGroup,\r\n    RepairHistory,\r\n    DropdownList,\r\n  \r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n.__services{\r\n    display: flex;\r\n    margin-bottom: 20px;\r\n}\r\n  \r\n.storage-group {\r\nmargin-bottom: 20px;\r\n}\r\n\r\n.group-title {\r\nfont-size: 18px;\r\nfont-weight: bold;\r\n}\r\n\r\n.btn-group {\r\nmargin-top: 10px;\r\n}\r\n\r\n.product-recycle {\r\ndisplay: flex;\r\njustify-content: space-between;\r\n}\r\n\r\n.product-info {\r\npadding: 20px;\r\nmargin-left: 50px;\r\nmargin-bottom: 20px;\r\n}\r\n\r\n.container {\r\ndisplay: flex;\r\njustify-content: center;\r\nmargin-top: 50px;\r\n}\r\n\r\n.centered-steps {\r\nwidth: 50%;\r\n}\r\n\r\n.left-panel {\r\nflex: 1;\r\nmargin-left: 400px;\r\nmargin-top: 100px;\r\n}\r\n\r\n.right-panel {\r\nright: 100px;\r\nflex: 2;\r\n}\r\n\r\n.gray-icon {\r\n    color: #888;\r\n    font-size: 18px;\r\n    margin-right: 5px;\r\n}\r\n\r\n.gray-text {\r\ncolor: #888;\r\nfont-size: 12px;\r\nmargin-right: 20px;\r\nmargin-bottom: 0px;\r\n}\r\n\r\n\r\n\r\nh1 {\r\nfont-size: 24px;\r\nfont-weight: bold;\r\n}\r\n\r\nh2 {\r\nfont-size: 18px;\r\n}\r\n\r\n.input{\r\nwidth: 300px;\r\n}\r\n\r\n.product-form {\r\nposition: relative;\r\nmargin-top: 0px;;\r\nmargin-left: 20px;\r\n}\r\n</style>\r\n  "],"mappings":"AAsFE,OAAOA,YAAW,MAAO,+BAA8B;AACvD,OAAOC,aAAY,MAAO,gCAA+B;AACzD,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,MAAK,MAAO,4BAA2B;AAE9C,eAAe;EACbC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,KAAK;MACXF,QAAQ,EAAE;IACZ;EACF,CAAC;EACDG,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAEC,OAAO,CAAC,eAAe,CAAC;MACtCC,WAAW,EAAE,MAAM;MACnBC,YAAY,EAAE,MAAM;MACpBC,IAAI,EAAE;QACJC,eAAe,EAAE,EAAE;QACnBC,eAAe,EAAE,EAAE;QACnBC,gBAAgB,EAAE,EAAE;QACpBC,aAAa,EAA
{"ast":null,"code":"import { Tickets, LocationInformation, LocationFilled, AddLocation, PhoneFilled, Help, InfoFilled, Setting, House } from '@element-plus/icons-vue';\nimport { reactive, ref } from 'vue';\nexport default {\n  components: {\n    Tickets: Tickets,\n    LocationInformation: LocationInformation,\n    LocationFilled: LocationFilled,\n    AddLocation: AddLocation,\n    PhoneFilled: PhoneFilled,\n    Help: Help,\n    InfoFilled: InfoFilled,\n    Setting: Setting,\n    House: House\n  },\n  setup: function setup() {\n    var ac_index = ref('1');\n    var Array = [{\n      id: '1',\n      icon: House,\n      title: '个人主页',\n      router: 'OwnPage',\n      Subclass: []\n    }, {\n      id: '2',\n      icon: Tickets,\n      title: '我的订单',\n      router: 'OrderCenter',\n      Subclass: []\n    }, {\n      id: '3',\n      icon: LocationInformation,\n      title: '邮寄地址',\n      router: 'MyAddress',\n      Subclass: []\n    }, {\n      id: '4',\n      icon: PhoneFilled,\n      title: '联系客服',\n      router: '',\n      Subclass: []\n    }, {\n      id: '5',\n      icon: Help,\n      title: '帮助中心',\n      router: '',\n      Subclass: []\n    }, {\n      id: '6',\n      icon: InfoFilled,\n      title: '关于我们',\n      router: '',\n      Subclass: []\n    }, {\n      id: '7',\n      icon: Setting,\n      title: '账户设置',\n      router: '',\n      Subclass: []\n    }];\n    var menu = reactive(Array);\n    return {\n      menu: menu,\n      ac_index: ac_index\n    };\n  }\n};","map":{"version":3,"names":["Tickets","LocationInformation","LocationFilled","AddLocation","PhoneFilled","Help","InfoFilled","Setting","House","reactive","ref","components","setup","ac_index","Array","id","icon","title","router","Subclass","menu"],"sources":["D:\\大二下\\数据库小学期\\gyy\\DBDesign-main\\src\\views\\home\\MainHome\\MainSider.vue"],"sourcesContent":["<template>\n    <!-- 侧边栏 -->\n    \n    <div class=\"sidebar-cont\">\n      <el-menu :default-active=\"ac_index\" @select=\"Select\">\n        <div v-for=\"(item, index) in menu\" :key=\"index\">\n          <router-link :to=\"{ path: item.router }\">\n            <el-menu-item v-if=\"item.Subclass.length == 0\" :index=\"item.id\">\n              <el-icon>\n                <component :is=\"item.icon\"></component>\n              </el-icon>\n              <span style=\"font-size:15px\">{{ item.title }}</span>\n            </el-menu-item>\n            <p></p>\n          </router-link>\n          <!-- 二级 -->\n          <el-sub-menu v-if=\"item.Subclass.length > 0\" :index=\"item.id\">\n            <template #title>\n              <el-icon>\n                <component :is=\"item.icon\"></component>\n              </el-icon>\n              <span  style=\"font-size:15px\">{{ item.title }}</span>\n            </template>\n            <div v-for=\"(two, index_two) in item.Subclass\" :key=\"index_two\">\n              <router-link :to=\"{ path: two.router }\">\n                <el-menu-item  style=\"font-size:12px\" :index=\"two.id\">{{ two.title }}</el-menu-item>\n              </router-link>\n            </div>\n          </el-sub-menu>\n          <p></p>\n        </div>\n      </el-menu>\n    </div>\n    <router-view></router-view>\n</template>\n\n<script>\nimport {\n  Tickets,\n  LocationInformation,\n  LocationFilled,\n  AddLocation,\n  PhoneFilled,\n  Help,\n  InfoFilled,\n  Setting,\n  House\n} from '@element-plus/icons-vue'\nimport { reactive, ref } from 'vue'\n\nexport default {\n  components: {\n    Tickets,\n    LocationInformation,\n    LocationFilled,\n    AddLocation,\n    PhoneFilled,\n    Help,\n    InfoFilled,\n    Setting,\n    House\n  },\n  setup() {\n    const ac_index = ref('1')\n\n    const Array = [\n    {\n        id: '1',\n        icon: House,\n        title: '个人主页',\n        router: 'OwnPage',\n        Subclass: []\n      },\n      {\n        id: '2',\n        icon: Tickets,\n        title: '我的订单',\n        router: 'OrderCenter',\n        Subclass: []\n      },\n      {\n        id: '3',\n        icon: LocationInformation,\n        title: '邮寄地址',\n        router: 'MyAddress',\n        Subclass: []\n      },\n      {\n        id: '4',\n        icon: PhoneFilled,\n        title: '联系客服',\n        router: '',\n        Subclass: []\n      },\n      {\n        id: '5',\n        icon: Help,\n        title: '帮助中心',\n        router: '',\n        Subclass: []\n      },\n      {\n        id: '6',\n        icon: InfoFilled,\n        title: '关于我们',\n        router: '',\n        Subclass: []\n      },\n      {\n        id: '7',\n        icon: Setting,\n        title: '账户设置',\n        router: '',\n        Subclass: []\n      }]\n    const menu = reactive(Array)\n\n\n    return { menu, ac_index }\n  }\n}\n</script>\n\n<style scoped>\n.sidebar-cont {\n  position: fixed;\n  top: 120px;\n  left: 0;\n  bottom: 50px;\n  background: #ffffff;\n  width: 9%;\n  height: 100vh;\n  color: #000000 !important;\n  overflow-y: auto;\n  /* 调整样式 */\n  background-color: #ffffff;\n\n}\n\n/* 调整菜单激活样式 */\n.el-menu-item--active {\n  background-color: #ffffff;\n}\n\n.router-link-active {\n  text-decoration: none;\n}\n\na {\n  text-decoration: none;\n}\n\n</style>"],"mappings":"AAqCA,SACEA,OAAO,EACPC,mBAAmB,EACnBC,cAAc,EACdC,WAAW,EACXC,WAAW,EACXC,IAAI,EACJC,UAAU,EACVC,OAAO,EACPC,KAAI,QACC,yBAAwB;AAC/B,SAASC,QAAQ,EAAEC,GAAE,QAAS,KAAI;AAElC,eAAe;EACbC,UAAU,EAAE;IACVX,OAAO,EAAPA,OAAO;IACPC,mBAAmB,EAAnBA,mBAAmB;IACnBC,cAAc,EAAdA,cAAc;IACdC,WAAW,EAAXA,WAAW;IACXC,WAAW,EAAXA,WAAW;IACXC,IAAI,EAAJA,IAAI;IACJC,UAAU,EAAVA,UAAU;IACVC,OAAO,EAAPA,OAAO;IACPC,KAAI,EAAJA;EACF,CAAC;EACDI,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,QAAO,GAAIH,GAAG,CAAC,GAAG;IAExB,IAAMI,KAAI,GAAI,CACd;MACIC,EAAE,EAAE,GAAG;MACPC,IAAI,EAAER,KAAK;MACXS,KAAK,EAAE,MAAM;MACbC,MAAM,EAAE,SAAS;MACjBC,QAAQ,EAAE;IACZ,CAAC,EACD;MACEJ,EAAE,EAAE,GAAG;MACPC,IAAI,EAAEhB,OAAO;MACbiB,KAAK,EAAE,MAAM;MACbC,MAAM,EAAE,aAAa;MACrBC,QAAQ,EAAE;IACZ,CAAC,EACD;MACEJ,EAAE,EAAE,GAAG;MACPC,IAAI,EAAEf,mBAAmB;MACzBgB,KAAK,EAAE,MAAM;MACbC,MAAM,EAAE,WAAW;MACnBC,QAAQ,EAAE;IAC
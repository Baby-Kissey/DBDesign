{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.array.find-index.js\";\nimport header from '/src/components/header.vue';\nexport default {\n  name: 'DetailsPage',\n  data: function data() {\n    return {\n      product: {\n        type: 'Phone',\n        name: 'iphone11',\n        description: '高性能手机',\n        price: '100$',\n        currentimageUrl: require('/public/p.jpg'),\n        // 图片路径根据实际情况进行调整\n        imageList: [{\n          id: 1,\n          url: require('/public/p.jpg')\n        },\n        // 图片路径根据实际情况进行调整\n        {\n          id: 2,\n          url: require('/public/p.jpg')\n        }, {\n          id: 3,\n          url: require('/public/p.jpg')\n        }, {\n          id: 4,\n          url: require('/public/p.jpg')\n        }, {\n          id: 5,\n          url: require('/public/p.jpg')\n        }],\n        srcList: ['/public/p.jpg', '/public/p.jpg', '/public/p.jpg', '/public/p.jpg', '/public/p.jpg']\n      },\n      activeIndex: 0\n    };\n  },\n  methods: {\n    goToRecyclePage: function goToRecyclePage() {\n      // 进入回收界面的逻辑\n      this.$router.push('/RecoveryPage');\n    },\n    goToRepairPage: function goToRepairPage() {\n      // 进入维修界面的逻辑\n      // 可以使用路由进行页面跳转或执行其他操作\n    },\n    changeImage: function changeImage(imageUrl) {\n      this.product.currentimageUrl = imageUrl;\n      this.activeIndex = this.product.imageList.findIndex(function (image) {\n        return image.url === imageUrl;\n      });\n    }\n  },\n  watch: {\n    activeIndex: function activeIndex(newIndex) {\n      this.product.currentimageUrl = this.product.imageList[newIndex].url;\n    }\n  },\n  components: {\n    \"seach\": header\n  }\n};","map":{"version":3,"names":["header","name","data","product","type","description","price","currentimageUrl","require","imageList","id","url","srcList","activeIndex","methods","goToRecyclePage","$router","push","goToRepairPage","changeImage","imageUrl","findIndex","image","watch","newIndex","components"],"sources":["D:\\nodejs\\DBDesign\\src\\views\\home\\DetailePage\\DetailsPage.vue"],"sourcesContent":["<template>\r\n<div>\r\n    <seach />\r\n\r\n\r\n    <el-steps :active=\"1\" class=\"centered-steps\">\r\n    <el-step title=\"预览\" description=\"选择您的设备\" />\r\n    <el-step title=\"回收\" description=\"设备信息\" />\r\n    <el-step title=\"选择\" description=\"Some description\" />\r\n    <el-step title=\"成功！\" description=\"Some description\" />\r\n    </el-steps>\r\n\r\n\r\n    <div class=\"product-details\">\r\n      \r\n      <div class=\"left-section\">\r\n        \r\n        <div class=\"display\">\r\n          <div class=\"product-image\">\r\n            <el-image\r\n            style=\"width: 300px; height: 300px;\"\r\n            :src=\"product.currentimageUrl\"\r\n            :zoom-rate=\"1.2\"\r\n            :preview-src-list=\"product.srcList\"\r\n            :initial-index=\"0\"\r\n            fit=\"cover\" \r\n            alt=\"Product Image\"\r\n          />\r\n          </div>\r\n        </div>\r\n  \r\n        <el-carousel :interval=\"4000\" type=\"card\" height=\"200px\" v-model=\"activeIndex\">\r\n          <el-carousel-item v-for=\"image in product.imageList\" :key=\"image.id\">\r\n            <img :src=\"image.url\" alt=\"Carousel Image\" @click=\"changeImage(image.url)\">\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n  \r\n      </div>\r\n  \r\n      <div class=\"right-section\">\r\n  \r\n        <el-card class=\"box-card\"> <!--  产品信息-->\r\n  \r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <h2 class=\"product-name\">{{ product.name }}</h2>\r\n            </div>\r\n          </template>\r\n  \r\n          <!-- <p class=\"product-description\">{{ product.description }}</p>    产品描述的表单 -->\r\n  \r\n          <!-- 产品具体描述信息 -->\r\n          <el-descriptions\r\n            direction=\"vertical\"\r\n            :column=\"4\"\r\n            :size=\"size\"\r\n            border\r\n          >\r\n  \r\n            <el-descriptions-item label=\"设备类别\">{{ product.type }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"设备型号\">{{ product.name }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"使用时长\">两年</el-descriptions-item>\r\n            <el-descriptions-item label=\"市场价格\">{{product.price}}</el-descriptions-item>\r\n  \r\n            \r\n  \r\n            <el-descriptions-item label=\"其他信息\"\r\n              >{{product.description}}\r\n            </el-descriptions-item>\r\n  \r\n          </el-descriptions>\r\n  \r\n  \r\n          <div class=\"container\">\r\n            <div class=\"button-group\">\r\n              <el-button class=\"button\" type=\"primary\" @click=\"goToRecyclePage\">回收界面</el-button>\r\n              <span style=\"width:90px;\"></span>\r\n              <el-button class=\"button\" type=\"success\" @click=\"goToRepairPage\">维修界面</el-button>\r\n            </div>\r\n          </div>\r\n  \r\n        </el-card>\r\n        \r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n  \r\n  <script>\r\n  import header from '/src/components/header.vue'\r\n  export default {\r\n    name:'DetailsPage',\r\n    data() {\r\n      return {\r\n        product: {\r\n          type:'Phone',\r\n          name: 'iphone11',\r\n          description: '高性能手机',\r\n          price:'100$',\r\n          currentimageUrl:require('/public/p.jpg'), // 图片路径根据实际情况进行调整\r\n          imageList: [\r\n            { id: 1, url: require('/public/p.jpg') }, // 图片路径根据实际情况进行调整\r\n            { id: 2, url: require('/public/p.jpg') }, \r\n            { id: 3, url: require('/public/p.jpg') }, \r\n            { id: 4, url: require('/public/p.jpg') }, \r\n            { id: 5, url: require('/public/p.jpg') } \r\n          ],\r\n          srcList : [\r\n          '/public/p.jpg',\r\n          '/public/p.jpg',\r\n          '/public/p.jpg',\r\n          '/public/p.jpg',\r\n          '/public/p.jpg',\r\n  ]\r\n        }, \r\n        activeIndex:0\r\n      };\r\n    },\r\n    methods: {\r\n      goToRecyclePage() {\r\n        // 进入回收界面的逻辑\r\n        this.$router.push('/RecoveryPage');\r\n      },\r\n      goToRepairPage() {\r\n        // 进入维修界面的逻辑\r\n        // 可以使用路由进行页面跳转或执行其他操作\r\n      },\r\n      changeImage(imageUrl){\r\n        this.product.currentimageUrl = imageUrl;\r\n        this.activeIndex = this.product.imageList.findIndex(image => image.url === imageUrl);\r\n      },\r\n  \r\n    },\r\n    watch:{\r\n      activeIndex(newIndex) {\r\n        this.product.currentimageUrl = this.product.imageList[newIndex].url;\r\n      }\r\n    },\r\n    components: {\r\n        \"seach\": header,\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  \r\n  \r\n  <style>\r\n  .left-section {\r\n    flex: 1;\r\n    margin-right: 20px;\r\n  }\r\n  \r\n  .container {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .display{\r\n    display: flex;\r\n    overflow-x: auto; /* 横向滚动 */\r\n    justify-content: center;\r\n    margin-top: 100px;\r\n    margin-bottom: 100px;\r\n  }\r\n  \r\n  .product-image {\r\n    width: 300px; /* 统一的正方形尺寸 */\r\n    height: 300px;\r\n    flex-shrink: 0; /* 防止缩小 */\r\n  }\r\n  \r\n  .product-image img{\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover; /* 填充容器并裁剪 */\r\n  }\r\n  \r\n  .box-card{\r\n    justify-content: center;\r\n    height: 600px;\r\n    width: 700px;\r\n    margin-top: 100px;\r\n    left: 10%;\r\n  }\r\n  \r\n  .product-details {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: flex-start;\r\n  }\r\n  \r\n  .el-carousel__item h3 {\r\n    color: #475669;\r\n    opacity: 0.75;\r\n    line-height: 200px;\r\n    margin: 0;\r\n    text-align: center;\r\n  }\r\n  \r\n  .el-carousel__item:nth-child(2n) {\r\n    background-color: #99a9bf;\r\n  }\r\n  \r\n  .el-carousel__item:nth-child(2n + 1) {\r\n    background-color: #d3dce6;\r\n  }\r\n  \r\n  .image-list {\r\n    display: flex;\r\n    overflow-x: auto; /* 横向滚动 */\r\n  }\r\n  \r\n  .image-item {\r\n    width: 200px; /* 统一的正方形尺寸 */\r\n    height: 200px;\r\n    margin-right: 10px; /* 图片间的间距 */\r\n    flex-shrink: 0; /* 防止缩小 */\r\n  }\r\n  \r\n  .image-item img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover; /* 填充容器并裁剪 */\r\n  }\r\n  \r\n  \r\n  \r\n  .right-section {\r\n    flex: 1;\r\n  }\r\n  \r\n  .el-descriptions {\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  .product-name {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .product-description {\r\n    text-align: center;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .container {\r\n    display: flex;\r\n    justify-content: center;\r\n    margin-top: 100px;\r\n  }\r\n  \r\n  .button-group {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .button{\r\n    width: 150px;\r\n    height: 75px;\r\n  }\r\n  \r\n  .demo-image__error .image-slot {\r\n    font-size: 30px;\r\n  }\r\n  .demo-image__error .image-slot .el-icon {\r\n    font-size: 30px;\r\n  }\r\n  .demo-image__error .el-image {\r\n    width: 100%;\r\n    height: 200px;\r\n  }\r\n  \r\n  </style>\r\n  "],"mappings":";;AA0FE,OAAOA,MAAK,MAAO,4BAA2B;AAC9C,eAAe;EACbC,IAAI,EAAC,aAAa;EAClBC,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,IAAI,EAAC,OAAO;QACZH,IAAI,EAAE,UAAU;QAChBI,WAAW,EAAE,OAAO;QACpBC,KAAK,EAAC,MAAM;QACZC,eAAe,EAACC,OAAO,CAAC,eAAe,CAAC;QAAE;QAC1CC,SAAS,EAAE,CACT;UAAEC,EAAE,EAAE,CAAC;UAAEC,GAAG,EAAEH,OAAO,CAAC,eAAe;QAAE,CAAC;QAAE;QAC1C;UAAEE,EAAE,EAAE,CAAC;UAAEC,GAAG,EAAEH,OAAO,CAAC,eAAe;QAAE,CAAC,EACxC;UAAEE,EAAE,EAAE,CAAC;UAAEC,GAAG,EAAEH,OAAO,CAAC,eAAe;QAAE,CAAC,EACxC;UAAEE,EAAE,EAAE,CAAC;UAAEC,GAAG,EAAEH,OAAO,CAAC,eAAe;QAAE,CAAC,EACxC;UAAEE,EAAE,EAAE,CAAC;UAAEC,GAAG,EAAEH,OAAO,CAAC,eAAe;QAAE,EACxC;QACDI,OAAM,EAAI,CACV,eAAe,EACf,eAAe,EACf,eAAe,EACf,eAAe,EACf,eAAe;MAEjB,CAAC;MACDC,WAAW,EAAC;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAe,WAAAA,gBAAA,EAAG;MAChB;MACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,eAAe,CAAC;IACpC,